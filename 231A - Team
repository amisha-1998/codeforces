#include<iostream>
using namespace std;
int main()
{
	int n;
	cin>>n;
	int p,v,t;
	int count=0;
	while(n--)
	{
		cin>>p>>v>>t;
		if(p==1&&v==1)
		{
			count=count+1;
		}
		else if(p==1&&t==1)
		{
			count=count+1;
		}
		else if(v==1&&t==1)
		{
			count=count+1;
		}
		
	}
	cout<<count<<endl;
	return 0;
}
